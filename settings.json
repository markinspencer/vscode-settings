{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "typescript.preferences.quoteStyle": "single",
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.jsxBracketSameLine": true,
  "terminal.integrated.fontFamily": "Meslo LG S, monospace",
  "terminal.integrated.fontSize": 10.5,
  "files.defaultLanguage": "javascript",
  "editor.cursorStyle": "underline",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorBlinking": "smooth",
  "editor.codeLens": false,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.fontFamily": "Ligconsolata, Office Code Pro, monospace",
  "editor.fontSize": 13,
  "editor.fontWeight": "500",
  "editor.letterSpacing": 0.6,
  "terminal.integrated.cursorBlinking": true,
  "editor.formatOnType": false,
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/obj": true,
    "**/*.js.map": true
  },
  "workbench.colorTheme": "Field Lights",
  "workbench.iconTheme": "field-lights",
  "editor.fontLigatures": true,
  "javascript.updateImportsOnFileMove.enabled": "always"
}
